sonar.projectKey=synpulse-group_{{sonar_project_key_suffix}}
sonar.organization=synpulse-group
sonar.sources=src
sonar.exclusions=**/node_modules/**,src/**/*.spec.ts,cypress/**/*,e2e/**/*,src/environments/**/*,src/test.ts,src/main.ts,src/karma.conf.js
